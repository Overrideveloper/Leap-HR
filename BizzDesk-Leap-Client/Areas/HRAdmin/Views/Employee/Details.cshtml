@model BizzDesk_Leap_Client.Areas.HRAdmin.Models.EmployeeClient

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title"><b>Employee Details</b></h4>
</div>

    <div class="modal-body">
        <dl class="dl-horizontal">
            @foreach (var deets in ViewBag.Details)
            {
                <dt> Employee ID: </dt>
                <dd> @deets.EmployeeID</dd>
                <dt> Last Name: </dt>
                <dd> @deets.LastName</dd>
                <dt> Middle Name: </dt>
                <dd> @deets.MiddleName</dd>
            }
        </dl>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-outline pull-left" data-dismiss="modal">Close</button>
    </div>

