@model BizzDesk_Leap_Client.Areas.HRAdmin.ViewModels.LeaveViewModel

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title"><b>Leave Type Constraints</b></h4>
</div>
    <div class="modal-body">
        <dl class="dl-horizontal">
                <dt> Gender </dt>
                <dd> @Html.DisplayFor(model => model.LeaveType.GenderConstraint)</dd>
                <dt> Department: </dt>
                <dd> @Html.DisplayFor(model => model.LeaveType.DepartmentConstraint.Title)</dd>
                <dt> Rank: </dt>
                <dd> @Html.DisplayFor(model => model.LeaveType.RankConstraint.Title)</dd>
        </dl>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-outline pull-left" data-dismiss="modal">Close</button>
    </div>

